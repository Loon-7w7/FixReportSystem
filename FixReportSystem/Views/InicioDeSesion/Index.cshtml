@{
    ViewData["Title"] = "Inicio de sesion";
}

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE-edge">
    <meta name="viewport" content="width-device-width, initial-scale-1.0">
    <link rel="stylesheet" href="/css/Estilos/EStilos formularios/estiloformulario.css">
</head>

<body>
    <div class="formulario">
        <h1>Inicio de Seseion</h1>
        <form method="post">
            <div class="Username">
                <input type="text" required>
                <label>Nombre de Usuario</label>
            </div>
            <div class="Username">
                <input type="password" required>
                <label>Contraseña</label>
            </div>
            <div class="Recordar"><a href="#">¿Olvideo su Contraseña?</a></div>
            <input type="submit" value="Iniciar">
            <div class="registrarse">
                Quiero hacer el <a href="#">Resgistro</a>
            </div>
        </form>
    </div>
</body>